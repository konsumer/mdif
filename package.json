{
  "name": "mdif",
  "type": "module",
  "version": "0.0.0",
  "description": "dialog/interactive fiction engine that lets you use markdown to describe conversations and games",
  "source": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx -y jest --verbose=true",
    "build": "npx -y microbundle",
    "start": "node examples/cli/adventure.js"
  },
  "exports": {
    "require": "./dist/mdif.cjs",
    "default": "./dist/mdif.modern.js"
  },
  "main": "./dist/mdif.cjs",
  "module": "./dist/mdif.module.js",
  "umd:main": "./dist/mdif.umd.js",
  "keywords": [
    "interactive",
    "fiction",
    "dialog",
    "game"
  ],
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT",
  "files": [
    "dist/mdif.cjs",
    "dist/mdif.cjs.map",
    "dist/mdif.modern.js",
    "dist/mdif.modern.js.map",
    "dist/mdif.module.js",
    "dist/mdif.module.js.map",
    "dist/mdif.umd.js",
    "dist/mdif.umd.js.map"
  ],
  "dependencies": {
    "did-you-mean": "^0.0.1",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-frontmatter": "^1.0.0",
    "memoizee": "^0.4.15",
    "micromark-extension-frontmatter": "^1.0.0",
    "mustache": "^4.2.0",
    "toml": "^3.0.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "chalk": "^5.0.1"
  }
}
